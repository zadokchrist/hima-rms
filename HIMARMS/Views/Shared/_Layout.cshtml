        <!--header end-->
        <!--sidebar start-->
        @Html.Partial("_SideBar")
        <!--sidebar end-->
        <!--main content start-->
        <section id="main-content">
            
            <section class="wrapper">
                @RenderBody()
            </section>
            <div class="text-right">
                <div class="credits">
